#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Quper/Project/nodejs/online_song/node_modules/.store/mime@4.0.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Quper/Project/nodejs/online_song/node_modules/.store/mime@4.0.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/mime@4.0.4/node_modules/mime/bin/cli.js" "$@"
else
  exec node  "$basedir/../.store/mime@4.0.4/node_modules/mime/bin/cli.js" "$@"
fi
